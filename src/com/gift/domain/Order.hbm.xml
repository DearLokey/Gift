<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gift.domain.Order" table="g_order">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="order_state" type="java.lang.String">
			<column name="ORDER_STATE"></column>
		</property>
		<property name="user_account" type="java.lang.String">
			<column name="USER_ACCOUNT"></column>
		</property>
		<property name="address_id" type="java.lang.Integer">
			<column name="ADDRESS_ID"></column>
		</property>
		<property name="order_time" type="java.sql.Timestamp">
			<column name="ORDER_TIME"></column>
		</property>
		<property name="gift_id" type="java.lang.Integer">
			<column name="GIFT_ID"></column>
		</property>
	</class>
</hibernate-mapping>